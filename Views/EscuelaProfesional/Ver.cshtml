@model SIGEM_TAEX.Models.Escuela_Profesional
@{
    ViewBag.Title = "Index";
}

<h2>Ver</h2>

<ol class="breadcrumb">
    <li><a href="~/EscuelaProfesional/Index">Escuela-Profesional</a></li>
    <li> / </li>
    <li class="active">@Model.ID_Escuela</li>
</ol>

<div class="card card-body">
    <div class="card-header">Informacion del la Escuela Profesional</div>
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.ID_Escuela</dd>

            <dt>Escuela Profesional</dt>
            <dd>@Model.Nombre_Escuela</dd>

            <dt>Facultad</dt>
            <dd>@Model.Facultad </dd>

            

        </dl>
    </div>
</div>
