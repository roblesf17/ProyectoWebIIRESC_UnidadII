@model SIGEM_TAEX.Models.Horario
@{
    ViewBag.Title = "Ver";
}

<h2>Ver</h2>

<ol class="breadcrumb">
    <li><a href="~/Horario/Index">Horario</a></li>
    <li> / </li>
    <li class="active">@Model.ID_Horario</li>
</ol>

<div class="card card-body">
    <div class="card-header">Informacion de Horarios</div>
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.ID_Horario</dd>

            <dt>Día</dt>
            <dd>@Model.Dia</dd>

            <dt>Hora</dt>
            <dd>@Model.Hora </dd>



        </dl>
    </div>
</div>

