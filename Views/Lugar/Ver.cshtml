}

<h2>Ver</h2>

<ol class="breadcrumb">
    <li><a href="~/Lugar/Index">Lugar</a></li>
    <li> / </li>
    <li class="active">@Model.ID_Lugar</li>
</ol>

<div class="card card-body">
