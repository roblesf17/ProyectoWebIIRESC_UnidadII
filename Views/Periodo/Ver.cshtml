@model SIGEM_TAEX.Models.Periodo
@{
    ViewBag.Title = "Ver";
}

<h2>Ver</h2>

<ol class="breadcrumb">
    <li><a href="~/Periodo/Index">Periodo</a></li>
    <li> / </li>
    <li class="active">@Model.ID_Periodo</li>
</ol>

<div class="card card-body">
