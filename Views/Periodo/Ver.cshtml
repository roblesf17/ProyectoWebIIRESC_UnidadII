    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.ID_Periodo</dd>

            <dt>Nombre del Periodo</dt>
            <dd>@Model.Nombre_Periodo</dd>

        </dl>
    </div>
</div>

