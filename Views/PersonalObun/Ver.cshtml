@model SIGEM_TAEX.Models.Personal_OBUN
@{
    ViewBag.Title = "Ver";
}


<div class="header bg-primary pb-6">
    <div class="container-fluid">
        <div class="header-body">
            <div class="row align-items-center py-4">
                <div class="col-lg-6 col-7">
                    <h6 class="h2 text-white d-inline-block mb-0">TAEX</h6>
                    <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
                        <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
                            <li class="breadcrumb-item">
                                <a href="#">
                                    <img src="~/recursos/imagen/home.png" width="20" height="20" style="margin-right:10px;">
                                </a>
                            </li>
                            <li class="breadcrumb-item"><a href="#">Personal OBUN</a></li>
                            <li class="breadcrumb-item active" aria-current="page"><a href="~/PersonalObun/Index">Lista</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Ver</li>
                            <li class="active">@Model.Nombres , @Model.Apellido_Paterno @Model.Apellido_Materno</li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>


<!-- Page content -->
<div class="container-fluid mt--6">
    <div class="row">
        <div class="col">
            <div class="card">
                <div class="card-header">
                    <div class="row align-items-center">
                        <div class="col-8">
                            <h3 class="mb-0">Personal OBUN </h3>
                        </div>
                    </div>
                </div>
                <div class="card-body">
                    <form>


                        <div class="card card-body">
                            <div class="card-header">Informacion del la Escuela Profesional</div>
                            <div class="card-body">
                                <dl class="dl-horizontal">
                                    <dt>Id</dt>
                                    <dd>@Model.ID_Personal</dd>

                                    <dt>DNI</dt>
                                    <dd>@Model.DNI</dd>

                                    <dt>Nombres</dt>
                                    <dd>@Model.Nombres </dd>

                                    <dt>Apellido Paterno</dt>
                                    <dd>@Model.Apellido_Paterno </dd>

                                    <dt>Apellido Materno</dt>
                                    <dd>@Model.Apellido_Materno </dd>

                                    <dt>Celular</dt>
                                    <dd>@Model.Celular </dd>

                                    <dt>Correo</dt>
                                    <dd>@Model.Correo </dd>

                                    <dt>Direccion</dt>
                                    <dd>@Model.Direccion </dd>

                                    <dt>Estado</dt>
                                    <dd>@(Model.Estado == "A" ? "Activo" : "Inactivo")</dd>

                                </dl>
                            </div>
                        </div>





                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- Footer -->
    <footer class="footer pt-0">
        <div class="row align-items-center justify-content-lg-between">
            <div class="col-lg-6">
                <div class="copyright text-center  text-lg-left  text-muted">
                    &copy; 2020 <a href="#" class="font-weight-bold ml-1" target="_blank">Taex - OBUN</a>
                </div>
            </div>
        </div>
    </footer>
</div>
